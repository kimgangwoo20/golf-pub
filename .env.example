# Firebase Configuration
FIREBASE_API_KEY=your_firebase_api_key_here
FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
FIREBASE_PROJECT_ID=your-project-id
FIREBASE_STORAGE_BUCKET=your-project.appspot.com
FIREBASE_MESSAGING_SENDER_ID=123456789012
FIREBASE_APP_ID=1:123456789012:android:abc123def456

# Kakao Login
KAKAO_NATIVE_APP_KEY=your_32_character_kakao_native_app_key_here
KAKAO_JAVASCRIPT_KEY=your_kakao_javascript_key_here

# Weather API (OpenWeatherMap)
WEATHER_API_KEY=your_openweathermap_api_key_here

# Toss Payments
TOSS_CLIENT_KEY=your_toss_payments_client_key_here
TOSS_SECRET_KEY=your_toss_payments_secret_key_here

# API Endpoints
API_BASE_URL=https://api.golfpub.kr
STAGING_API_URL=https://staging-api.golfpub.kr

# Feature Flags
ENABLE_CHAT=true
ENABLE_MUSIC=false
ENABLE_DEBUG=false

# Environment
NODE_ENV=development
APP_VARIANT=development
